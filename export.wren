class Emoji {
  static favicon(emoji) { "<link href=\"data:image/svg+xml,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 16 16'><text y='14' x='0'>%( emoji )</text></svg>\" rel=\"icon\">" }
  static heart { "❤️" }
  static star { "⭐️" }
  static fire { "🔥" }
  static rocket { "🚀" }
  static sparkles { "✨" }
  static zap { "⚡️" }
  static check { "✅" }
  static cross { "❌" }
  static tada { "🎉" }
  static emergency { "🚨" }
  static joy { "😂" }
  static wink { "😉" }
  static foldedHands { "🙏" }
  static thumbsUp { "👍" }
  static thumbsDown { "👎" }
}
